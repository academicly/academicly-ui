{
  "name": "academicly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage",
    "test:onlyChanged": "craco test --onlyChanged",
    "test:ci": "CI=true craco test",
    "test:ci:coverage": "CI=true craco test --coverage",
    "test:ci:onlyChanged": "CI=true craco test --onlyChanged",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx,js,jsx}' --fix",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -c .storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "babel": {
    "presets": [
      "linaria/babel"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "antd": "^3.16.2",
    "js-cookie": "^2.2.0",
    "react": "next",
    "react-dom": "next",
    "react-onclickout": "^2.0.8",
    "react-scripts": "3.0.1",
    "react-spring": "^9.0.0-beta.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@craco/craco": "^4.1.0",
    "@storybook/addon-actions": "next",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-links": "next",
    "@storybook/addons": "next",
    "@storybook/react": "next",
    "@types/jest": "^24.0.11",
    "@types/js-cookie": "^2.2.2",
    "@types/node": "^11.13.2",
    "@types/reach__router": "^1.2.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-loader": "8.0.5",
    "core-js": "^2.6.5",
    "craco-antd": "^1.10.0",
    "craco-less": "^1.7.0",
    "css-loader": "^2.1.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "less-loader": "^5.0.0",
    "linaria": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "react-docgen-typescript-loader": "^3.1.0",
    "style-loader": "^0.23.1",
    "typescript": "^3.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix",
      "git add"
    ]
  }
}
